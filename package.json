  "description": "",
  "main": "start.js",
  "scripts": {
    "start": "npx nodemon start.js"
  },
  "author": "william341",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.626.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cookies": "^0.8.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.1.0",
    "mongoose": "^5.8.10",
    "multiparty": "^4.2.1",
    "passport": "^0.4.1",
    "passport-local-mongoose": "^6.0.1",
    "random-number-csprng": "^1.0.2",
    "request": "^2.88.2",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.2.0",
    "socketio-jwt": "^4.5.0",
    "uuid": "^3.4.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "es2017": true
    },
    "rules": {
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "curly": [
        "error",
        "multi-line"
      ],
      "eqeqeq": [
        "error",
        "always"
      ],
      "no-invalid-this": [
        "error"
      ],
      "array-bracket-spacing": [
        "error",
        "never"
      ],
      "block-spacing": [
        "error",
        "never"
      ],
      "brace-style": [
        "error",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-var": "error"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2"
  }
}
